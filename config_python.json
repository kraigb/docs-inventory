{
    "content": [
        {
            "repo": "MicrosoftDocs/azure-docs-pr",
            "path": "${INVENTORY_REPO_ROOT}\\azure-docs-pr",
            "url": "http://docs.microsoft.com/azure",
            "exclude_folders" : ["includes", "markdown templates"]
        },
        {
            "repo": "MicrosoftDocs/vsts-docs-pr",
            "path": "${INVENTORY_REPO_ROOT}\\vsts-docs-public",
            "url": "https://docs.microsoft.com/azure/devops",
            "exclude_folders" : ["includes"]
        },
        {
            "repo": "MicrosoftDocs/visualstudio-docs-pr/docs",
            "path": "${INVENTORY_REPO_ROOT}\\visualstudio-docs-pr\\docs",
            "url": "https://docs.microsoft.com/visualstudio",
            "exclude_folders" : ["includes", "vs-2015", "extensibility", "rtvs"]
        },
        {
            "repo": "Microsoft/vscode-docs",
            "path": "${INVENTORY_REPO_ROOT}\\vscode-docs",
            "url": "https://code.visualstudio.com/docs",
            "exclude_folders" : ["includes", "release-notes", "blogs"]
        }, 
        {
            "repo": "MicrosoftDocs/azure-docs-sdk-python",
            "path": "${INVENTORY_REPO_ROOT}\\azure-docs-sdk-python",
            "url": "https://docs.microsoft.com/python/azure",
            "exclude_folders" : ["includes"]
        }
    ],
    "inventory": [
        {
            "name": "Python",
            "terms": [
                "Python",
                "Flask",
                "Django"
            ]
        },
        {
            "name": "JupyterML",
            "terms": [
                "Jupyter",
                "Spark",
                "data science",
                "data-science",
                "machine learning",
                "machine-learning"
            ]
        }
    ]
}
